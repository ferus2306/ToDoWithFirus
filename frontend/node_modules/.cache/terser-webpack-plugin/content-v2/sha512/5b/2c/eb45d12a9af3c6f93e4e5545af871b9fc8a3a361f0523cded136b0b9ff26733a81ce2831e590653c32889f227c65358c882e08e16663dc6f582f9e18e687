: \"\",\n        task: \"\",\n        completed: false,\n        pending: false\n    });\n\n\n    function handleSubmit(e) {\n        // prevents from default submission\n        e.preventDefault();\n        if (todo.task.trim()) {\n            addTodo({ ...todo, id: uuidv4() });\n            // reset task input\n            setTodo({ ...todo, task: \"\"})\n        }\n    }\n\n    function handleTaskInputChange(e) {\n        setTodo({ ...todo, task: e.target.value})\n    }\n\n\n    return (\n        <Form onSubmit={handleSubmit}>\n        <Form.Group>\n            <Form.Control\n            type=\"text\"\n            placeholder=\"Enter To Do Item\" \n            value={todo.task}\n            onChange={handleTaskInputChange}\n            />\n        </Form.Group>\n\n            <Button variant=\"primary\" size=\"sm\" type=\"submit\">\n                Add\n            </Button>\n        </Form>\n  );\n}\n\n\nexport default TodoForm\n\n","// import React, { useState } from 'react'\nimport { Button, Col, Row } from 'react-bootstrap'\n\nfunction Todo({ todo, index, deleteTodo, editTodo }) {\n  // console.log(index);\n  return (\n    <Row className=\"todo py-2 px-4 border\">\n      <Col xs={2}>{index + 1}</Col>\n      <Col >{todo.text}</Col>\n\n      \n       {/* DELETE */}\n      <Button\n        className=\"mx-2\"\n        size=\"sm\"\n        variant=\"danger\"\n        onClick={() => {\n          deleteTodo(index)\n        }}>Delete</Button>\n      \n\n      {/* EDIT */}\n      <Button\n        className=\"mx-2\"\n        size=\"sm\"\n        variant=\"warning\"\n        onClick={() => {\n          editTodo(index, todo.text)\n        }}\n      >Edit</Button>\n      \n\n      {/* COMPLETE */}\n        <Button\n        className=\"mx-2\"\n        size=\"sm\"\n        variant=\"success\"\n\n        >Complete</Button>\n    </Row>\n  );\n};\n\n\nexport default Todo\n\n","import './App.css';\nimport React from 'react';\nimport Header from './components/Header.js'\nimport 